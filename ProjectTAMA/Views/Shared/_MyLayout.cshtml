    <div id="colorNav">
        <div id="wrap">
            <div id="main">

                <nav class="colorlib-nav" role="navigation">
                    <div class="top-menu">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-xs-2">
                                    <div id="colorlib-logo"></div>
                                    <h3>@Html.ActionLink("TAMA", "Index", "Home")</h3>


                                </div>

                                <div class="col-xs-10 text-right menu-1">
                                    <ul>
                                        <li>Welcome @HttpContext.Current.Session["UserName"]</li>
                                        @*<h3> @HttpContext.Current.Session["UserCountry"]</h3>*@
                                        <li>@Html.ActionLink("Home", "Index", "Home", null, new { @area = "" })</li>

                                        <li>@Html.ActionLink("Places", "Index", "Places", null, new { @area = "" })</li>


                                        <li>@Html.ActionLink("Hotels", "Hotels", "Home", null, new { @area = "" })</li>
                                        @if (HttpContext.Current.Session["UserName"] != null)
                                        {


                                            <li>@Html.ActionLink("Recommendation", "Recommendations", "Home", null, new { @area = "" })</li>

                                            <li>@Html.ActionLink("Log out", "LogOff", "Home", null, new { @area = "" })</li>


                                        }
                                        @if (HttpContext.Current.Session["admin"] != null)
                                        {

                                            <li class="has-dropdown">
                                                <a href="#">Admin</a>
                                                <ul class="dropdown">
                                                    <li>@Html.ActionLink("Place Info", "Admin", "Home", null, new { @area = "" })</li>
                                                    <li>@Html.ActionLink("Hotel Info", "HotelAdmin", "Home", null, new { @area = "" })</li>
                                                    <li>@Html.ActionLink("Log out", "LogOff", "Home", null, new { @area = "" })</li>
                                                    

                                                </ul>
                                            </li>

                                        }

                                        @*<li>@Html.ActionLink("Admin", "Admin", "Home", null, new { @area = "" })</li>*@

                                    </ul>







                                </div>

                            </div>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
