<!DOCTYPE html>
<html>
<head>
    <title>@Page.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Tour Template</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <meta name="author" content="" />
    <!-- Facebook and Twitter integration -->
    <meta property="og:title" content="" />
    <meta property="og:image" content="" />
    <meta property="og:url" content="" />
    <meta property="og:site_name" content="" />
    <meta property="og:description" content="" />
    <meta name="twitter:title" content="" />
    <meta name="twitter:image" content="" />
    <meta name="twitter:url" content="" />
    <meta name="twitter:card" content="" />
    <link href="https://fonts.googleapis.com/css?family=Quicksand:300,400,500,700" rel="stylesheet">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">


    @RenderSection("head", required: false)




</head>
<body>
    <div id="colorNav">
        <div id="wrap">
            <div id="main">

                <nav class="colorlib-nav" role="navigation">
                    <div class="top-menu">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-xs-2">
                                    <div id="colorlib-logo"></div>
                                    <li>@Html.ActionLink("TAMA", "Index", "Home")</li>
                                </div>
                                <div class="col-xs-10 text-right menu-1">
                                    <ul>
                                        <li>@Html.ActionLink("Home", "Index", "Home", null, new { @area = "" })</li>

                                        <li>@Html.ActionLink("Places", "Index", "Places", null, new { @area = "" })</li>


                                        <li>@Html.ActionLink("Hotels", "Hotels", "Home", null, new { @area = "" })</li>
                                        <li>@Html.ActionLink("Admin", "Index", "PlaceTables", null, new { @area = "" })</li>


                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
    </div>
    <aside id="colorlib-hero">
        <div class="flexslider">
            <ul class="slides">
                <li style="background-image: url(images/khaptad.JPG);">
                    <div class="overlay"></div>
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                                <div class="slider-text-inner text-center">

                                    <h1>Khaptad</h1>
                                    <h2>The untouched land.</h2>
                                </div>
                            </div>
                        </div>
                    </div>
                </li>
                <li style="background-image: url(images/Rara.JPG);">
                    <div class="overlay"></div>
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                                <div class="slider-text-inner text-center">
                                    <h2>Rara lake</h2>
                                    <h2>Place where heaven meets earth.</h2>
                                </div>
                            </div>
                        </div>
                    </div>
                </li>
                <li style="background-image: url(images/ABC.jpeg);">
                    <div class="overlay"></div>
                    <div class="container-fluids">
                        <div class="row">
                            <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                                <div class="slider-text-inner text-center">
                                    <h1>Annapurna Region</h1>
                                    <h2>The beauty within</h2>
                                </div>
                            </div>
                        </div>
                    </div>
                </li>
                <li style="background-image: url(images/everest.jpg);">
                    <div class="overlay"></div>
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                                <div class="slider-text-inner text-center">
                                    <h1>Everest Region</h1>
                                    <h2>Top of the world.</h2>
                                </div>
                            </div>
                        </div>
                    </div>
                <li style="background-image: url(images/pokhara.jpg);">
                    <div class="overlay"></div>
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                                <div class="slider-text-inner text-center">
                                    <h1>Pokhara</h1>
                                    <h2>Best place to chill.</h2>
                                </div>
                            </div>
                        </div>
                    </div>
                </li>
            </ul>
        </div>
    </aside>
    <div class="container body-content">
        @RenderBody()



        <class id="footer">
            <footer id="colorlib-footer" role="contentinfo">
                <div class="row row-pb-md">
                    <div class="col-md-3 mb-5 pr-md-5 contact-info">
                        <h4>Contact Information</h4>
                        <p><span class="d-block">Address:</span> <span>Subidhanagar, Tinkune, Kathmandu</span></p>
                        <p><span class="d-block">Phone:</span> <span>(+977)-9810116860</span></p>
                        <p><span class="d-block">Email:</span> <span> bisowray1069@gmail.com</span></p>
                    </div>

                    <div class="row">
                        <div class="col-md-12 text-center">
                            <p>
                                <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                                Copyright &copy;
                                <script>document.write(new Date().getFullYear());</script> All rights reserved | This website belongs to TAMA group <i class="icon-heart2" aria-hidden="true"></i>
                            </p>
                        </div>
                    </div>
                </div>
            </footer>
        </class>
    </div>



    <style>
        body {
            margin: 0 0 20px;
        }

        .footer {
            position: fixed;
            bottom: 0;
            width: 100%;
        }
    </style>

    <!-- Animate.css -->
    <link rel="stylesheet" href=@Url.Content("Theme/tour/css/animate.css")>
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href=@Url.Content("Theme/tour/css/icomoon.css")>
    <!-- Bootstrap  -->
    <link rel="stylesheet" href=@Url.Content("Theme/tour/css/bootstrap.css")>
    <!-- Magnific Popup -->
    <link rel="stylesheet" href=@Url.Content("Theme/tour/css/magnific-popup.css")>
    <!-- Flexslider  -->
    <link rel="stylesheet" href=@Url.Content("Theme/tour/css/flexslider.css")>
    <!-- Owl Carousel -->
    <link rel="stylesheet" href=@Url.Content("Theme/tour/css/owl.carousel.min.css")>
    <link rel="stylesheet" href=@Url.Content("Theme/tour/css/owl.theme.default.min.css")>

    <!-- Date Picker -->
    <link rel="stylesheet" href=@Url.Content("Theme/tour/css/bootstrap-datepicker.css")>
    <!-- Flaticons  -->
    <link rel="stylesheet" href=@Url.Content("Theme/tour/fonts/flaticon/font/flaticon.css")>
    <!-- Theme style  -->
    <link rel="stylesheet" href=@Url.Content("Theme/tour/css/style.css")>
    <!-- Modernizr JS -->
    <script src=@Url.Content("Theme/tour/js/modernizr-2.6.2.min.js")></script>
    <!-- FOR IE9 below -->
    <!--[if lt IE 9]>
    <script src="js/respond.min.js"></script>
    <![endif]-->
    <!-- jquery and bootstrap section-->
    <!-- jQuery -->
    <script src=@Url.Content("Theme/tour/js/jquery.min.js")></script>
    <!-- jQuery Easing -->
    <script src=@Url.Content("Theme/tour/js/jquery.easing.1.3.js")></script>
    <!-- Bootstrap -->
    <script src=@Url.Content("Theme/tour/js/bootstrap.min.js")></script>
    <!-- Waypoints -->
    <script src=@Url.Content("Theme/tour/js/jquery.waypoints.min.js")></script>
    <!-- Flexslider -->
    <script src=@Url.Content("Theme/tour/js/jquery.flexslider-min.js")></script>
    <!-- Owl carousel -->
    <script src=@Url.Content("Theme/tour/js/owl.carousel.min.js")></script>
    <!-- Magnific Popup -->
    <script src=@Url.Content("Theme/tour/js/jquery.magnific-popup.min.js")></script>
    <script src=@Url.Content("Theme/tour/js/magnific-popup-options.js")></script>
    <!-- Date Picker -->
    <script src=@Url.Content("Theme/tour/js/bootstrap-datepicker.js")></script>
    <!-- Stellar Parallax -->
    <script src=@Url.Content("Theme/tour/js/jquery.stellar.min.js")></script>
    <!-- Main -->
    <script src=@Url.Content("Theme/tour/js/main.js")></script>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)




</body>
</html>
